<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
http://www.springframework.org/schema/context
http://www.springframework.org/schema/context/spring-context-3.0.xsd">
	<context:annotation-config />

	<bean id="profile" class="com.aug.jma.domain.Profiel"></bean>
	<bean id="student1" class="com.aug.jma.domain.Student">
	   <property name="name" value="student1"></property>
	   <property name="age" value="11"></property>
	</bean>

	<bean id="student2" class="com.aug.jma.domain.Student">
       <property name="name" value="student2"></property>
       <property name="age" value="22"></property>
    </bean>
</beans>